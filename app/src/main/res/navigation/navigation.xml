<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="edu.neu.madcourse.testmusic.MainFragment"
        android:label="Home"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_navigation_send_to_navigation_receive"
            app:destination="@+id/navigation_home"/>
    </fragment>

    <fragment
        android:id="@+id/navigation_leaderboard"
        android:name="edu.neu.madcourse.testmusic.LeaderboardFragment"
        android:label="Home"
        tools:layout="@layout/fragment_leaderboard">
        <action
            android:id="@+id/action_navigation_receive_to_navigation_history"
            app:destination="@+id/navigation_leaderboard"/>
    </fragment>

    <fragment
        android:id="@+id/navigation_aboutme"
        android:name="edu.neu.madcourse.testmusic.AboutMeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_about_me"/>

</navigation>